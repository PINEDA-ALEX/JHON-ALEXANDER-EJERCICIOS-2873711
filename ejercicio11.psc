Algoritmo sin_titulo
	//definir variables
	Definir grados_centigrados, grados_fahrenheit como Real;
	
	//ingrsar los datos 
    Escribir "Ingrese los grados Celsius: ";
    Leer grados_centigrados;
	//operacion de grados celsios a fahrenheit
    grados_fahrenheit <- grados_centigrados * 1.8 + 32;
	//salida de los datoas
    Escribir "El resultado en grados Fahrenheit es: ", grados_fahrenheit;

FinAlgoritmo
